{
  "name": "discord-bot-template",
  "version": "1.0.0",
  "description": "Discord bot created from a TypeScript template",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node .",
    "dev": "nodemon --config nodemon.json src/index.ts",
    "deploy": "cross-env IS_SCRIPT=true ts-node src/scripts/deploy",
    "deploy-prod": "cross-env NODE_ENV=production npm run deploy",
    "clear": "cross-env IS_SCRIPT=true ts-node src/scripts/clear",
    "format": "prettier --write src"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.3.3",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.44.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "discord.js": "^14.11.0",
    "dotenv": "^16.3.1"
  }
}
